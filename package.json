{
  "name": "yarn-website",
  "version": "1.0.0",
  "devDependencies": {
    "algolia-sitemap": "^2.0.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "happypack": "^4.0.0-beta.2",
    "lint-staged": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "webpack": "^3.5.5",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "scripts": {
    "build": "webpack",
    "build:sitemaps": "./scripts/sitemaps.js",
    "build:highlight.js": "./scripts/highlight.sh",
    "build:production": "npm run build:highlight.js && npm run build && npm run build:sitemaps",
    "start": "npm run build:highlight.js && webpack --watch",
